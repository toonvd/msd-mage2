{
	"name": "magestackday/msd-m2",
	"description": "MageStackDay webshop on Magento 2.0",
    "license": "OSL-3.0",
    "type": "project",
    "authors": [
        {
            "name": "Sander Mangel",
            "email": "sander@sandermangel.nl",
            "homepage": "http://www.twitter.com/sandermangel",
            "role": "Maintainer"
        }
    ],
    "require": {
        "magento/product-community-edition": "2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Setup\\": "setup/src/Magento/Setup/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "chmod ug+x bin/magento"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.magento.com/"
        }
    ]
}
