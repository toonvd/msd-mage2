{
    "name": "magestackday/msd-m2",
    "description": "MageStackDay webshop on Magento 2.0",
    "license": "OSL-3.0",
    "type": "project",
    "authors": [
        {
            "name": "Sander Mangel",
            "email": "sander@sandermangel.nl",
            "homepage": "http://www.twitter.com/sandermangel",
            "role": "Maintainer"
        }
    ],
    "require": {
        "magento/project-community-edition": "2.0.1",
        "sama/skipcart": "*",
        "msd/theme-frontend-default": "*",
        "msd/statistics": "*"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Setup\\": "setup/src/Magento/Setup/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "chmod ug+x bin/magento"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type":"git",
            "url": "https://github.com/sandermangel/magento2-skipcart.git"
        },
        {
            "type":"git",
            "url":"https://github.com/toonvd/msdtheme.git"
        },
        {
            "type":"git",
            "url":"https://github.com/magestackday/msd-statistics.git"
        }
    ]
}

